{
	"scripts": {
		"dev": "cross-env ENV=development PORT=3000 IO_PORT=3030 webpack serve --mode=development",
		"prod": "webpack --mode=production",
		"prod:esm": "cross-env ESM=true webpack --mode=production",
		"serve": "cross-env PORT=8080 NODE_NO_WARNINGS=1 node ./config/webpack.serve.config.js",
		"prettier": "tsc --noEmit && prettier src/**/*.{t,j}s{,x} --no-error-on-unmatched-pattern --check",
		"lint": "tsc --noEmit && eslint src/**/*.{t,j}s{,x} --no-error-on-unmatched-pattern --ignore-pattern node_modules/",
		"lint:fix": "npm run lint -- --fix",
		"prettier:fix": "npm run prettier -- --write",
		"format": "npm run prettier:fix && npm run lint:fix",
		"prepare": "npx husky install",
		"pre-commit": "lint-staged"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	],
	"overrides": {
		"isomorphic-fetch@*": "$isomorphic-fetch"
	},
	"devDependencies": {
		"@babel/core": "^7.20.5",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.18.6",
		"@commitlint/config-conventional": "^17.3.0",
		"@rushstack/eslint-patch": "^1.2.0",
		"@typescript-eslint/eslint-plugin": "^5.47.0",
		"@typescript-eslint/parser": "^5.47.0",
		"@vue/eslint-config-prettier": "^7.0.0",
		"@vue/eslint-config-typescript": "^11.0.2",
		"autoprefixer": "^10.4.13",
		"babel-loader": "^9.1.0",
		"babel-preset-typescript-vue3": "^2.0.17",
		"clean-webpack-plugin": "^4.0.0",
		"commitizen": "^4.2.6",
		"commitlint": "^17.3.0",
		"core-js": "^3.26.1",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.3",
		"css-minimizer-webpack-plugin": "^4.2.2",
		"esbuild-loader": "^2.20.0",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-custom-alias": "^1.3.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.8.0",
		"git-cz": "^4.9.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^8.0.2",
		"isomorphic-fetch": "^3.0.0",
		"lint-staged": "^13.1.0",
		"mini-css-extract-plugin": "^2.7.2",
		"nodemon": "^2.0.20",
		"postcss": "^8.4.20",
		"postcss-loader": "^7.0.2",
		"postcss-preset-env": "^7.8.3",
		"prettier": "^2.8.1",
		"quicktype-core": "^6.1.0",
		"sass": "^1.57.1",
		"sass-loader": "^13.2.0",
		"socket.io": "^4.5.4",
		"tailwindcss": "^3.2.4",
		"terser-webpack-plugin": "^5.3.6",
		"unplugin-auto-import": "^0.12.1",
		"vue-loader": "^17.0.1",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^4.11.1"
	},
	"dependencies": {
		"vue": "^3.2.45",
		"vue-router": "^4.1.6"
	}
}
