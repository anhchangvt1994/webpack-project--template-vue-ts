{
	"scripts": {
		"dev": "cross-env ENV=development PORT=3000 IO_PORT=3030 webpack serve --mode=development",
		"build": "vue-tsc && webpack --mode=production",
		"build:esm": "vue-tsc && cross-env ESM=true webpack --mode=production",
		"preview": "cross-env PORT=8080 NODE_NO_WARNINGS=1 node ./config/webpack.serve.config.js",
		"prettier": "vue-tsc && prettier src/**/*.{vue,js,ts}{,x} --no-error-on-unmatched-pattern --check",
		"lint": "vue-tsc && eslint src/**/*.{vue,js,ts}{,x} --no-error-on-unmatched-pattern --ignore-pattern node_modules/",
		"lint:fix": "npm run lint -- --fix",
		"prettier:fix": "npm run prettier -- --write",
		"format": "npm run prettier:fix && npm run lint:fix",
		"prepare": "git config core.autocrlf false && npx husky install",
		"pre-commit": "vue-tsc && lint-staged"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	],
	"overrides": {
		"isomorphic-fetch@*": "$isomorphic-fetch",
		"tsconfig-paths@*": "$tsconfig-paths"
	},
	"devDependencies": {
		"@babel/core": "^7.21.8",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/preset-env": "^7.21.5",
		"@babel/preset-typescript": "^7.21.5",
		"@commitlint/config-conventional": "^17.6.3",
		"@rushstack/eslint-patch": "^1.2.0",
		"@swc/core": "^1.3.57",
		"@typescript-eslint/eslint-plugin": "^5.59.5",
		"@typescript-eslint/parser": "^5.59.5",
		"@vue/eslint-config-prettier": "^7.1.0",
		"@vue/eslint-config-typescript": "^11.0.3",
		"autoprefixer": "^10.4.14",
		"babel-loader": "^9.1.2",
		"babel-preset-typescript-vue3": "^2.0.17",
		"clean-webpack-plugin": "^4.0.0",
		"commitizen": "^4.3.0",
		"commitlint": "^17.6.3",
		"copy-webpack-plugin": "^11.0.0",
		"core-js": "^3.30.2",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.3",
		"css-minimizer-webpack-plugin": "^5.0.0",
		"cz-git": "^1.6.1",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-custom-alias": "^1.3.2",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.12.0",
		"espree": "^9.5.2",
		"html-webpack-plugin": "^5.5.1",
		"husky": "^8.0.3",
		"isomorphic-fetch": "^3.0.0",
		"lint-staged": "^13.2.2",
		"mini-css-extract-plugin": "^2.7.5",
		"nodemon": "^2.0.22",
		"postcss": "^8.4.23",
		"postcss-loader": "^7.3.0",
		"postcss-preset-env": "^8.3.2",
		"prettier": "^2.8.8",
		"purgecss-webpack-plugin": "^5.0.0",
		"quicktype-core": "^23.0.30",
		"sass": "^1.62.1",
		"sass-loader": "^13.2.2",
		"socket.io": "^4.6.1",
		"swc-loader": "^0.2.3",
		"tailwindcss": "^3.3.2",
		"terser-webpack-plugin": "^5.3.8",
		"tsconfig-paths": "^4.2.0",
		"unplugin-auto-import": "^0.15.3",
		"vue-eslint-parser": "^9.3.0",
		"vue-loader": "^17.1.1",
		"vue-tsc": "^1.6.5",
		"webpack": "^5.82.1",
		"webpack-cli": "^5.1.1",
		"webpack-dev-server": "^4.15.0"
	},
	"dependencies": {
		"vue": "^3.3.2"
	},
	"eslintConfig": {
		"extends": [
			"./config/eslint.config.js"
		]
	},
	"prettier": "./config/prettier.config.js",
	"lint-staged": {
		"*.{vue,js,jsx,ts,tsx}": "npm run prettier:fix && npm run lint"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		],
		"rules": {
			"type-enum": [
				2,
				"always",
				[
					"feat",
					"style",
					"test",
					"fix",
					"docs",
					"release",
					"update",
					"perf",
					"chore"
				]
			]
		}
	},
	"config": {
		"commitizen": {
			"path": "cz-git",
			"czConfig": "./config/cz.config.js"
		}
	}
}
